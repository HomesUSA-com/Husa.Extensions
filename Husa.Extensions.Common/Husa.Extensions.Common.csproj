<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <AssemblyName>Husa.Extensions.Common</AssemblyName>
        <RootNamespace>Husa.Extensions.Common</RootNamespace>
        <Product>Common Extensions</Product>
        <Description>Husa Common</Description>
        <PackageId>Husa.Extensions.Common</PackageId>
        <Version>2.4.0</Version>
        <PackageTags>Extensions; Common;</PackageTags>
        <PackageReleaseNotes>
            Release 2.4.0:
            - Added CommandResult class for passing result information between layers
            - Included the enum ResponseCode as part of the CommandResult class
        </PackageReleaseNotes>
        <IsPackable>true</IsPackable>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.AspNetCore.WebUtilities" Version="2.2.0" />
        <PackageReference Include="AutoMapper" Version="11.0.1" />
        <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="6.0.1" />
        <PackageReference Include="Microsoft.AspNetCore.Mvc.Core" Version="2.2.5" />
        <PackageReference Include="Microsoft.AspNetCore.Mvc.NewtonsoftJson" Version="6.0.6" />
    </ItemGroup>

</Project>
