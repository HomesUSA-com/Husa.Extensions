<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <AssemblyName>Husa.Extensions.Downloader.RetsSdk</AssemblyName>
        <RootNamespace>Husa.Extensions.Downloader.RetsSdk</RootNamespace>
        <Product>HomesUSA Rets SDK</Product>
        <Description>.Client library for interacting with a RETS server to pull real estate listings, photos and other data made available from an MLS system.</Description>
        <PackageId>Husa.Extensions.Downloader.RetsSdk</PackageId>
        <PackageTags>Extensions; Rets; RetsSDK; Downloader</PackageTags>
        <Version>3.1.0</Version>
        <PackageReleaseNotes>
            Release 3.1.0:
            - Market Limit added to Connection Options.
            - RetsClient Search: Pagination added using market limit.
        </PackageReleaseNotes>
        <IsPackable>true</IsPackable>
    </PropertyGroup>
    
    <ItemGroup>
        <PackageReference Include="Microsoft.Extensions.Http" Version="6.0.0" />
        <PackageReference Include="Microsoft.Extensions.Logging" Version="6.0.0" />
        <PackageReference Include="MimeKit" Version="3.3.0" />
        <PackageReference Include="MimeTypeMap.Core" Version="1.0.0" />
        <PackageReference Include="X.PagedList" Version="8.4.3" />
    </ItemGroup>
</Project>
