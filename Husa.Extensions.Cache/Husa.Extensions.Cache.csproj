<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <AssemblyName>Husa.Extensions.Cache</AssemblyName>
        <RootNamespace>Husa.Extensions.Cache</RootNamespace>
        <Product>Cache Extensions</Product>
        <Description>Husa Cache</Description>
        <PackageId>Husa.Extensions.Cache</PackageId>
        <Version>2.1.0</Version>
        <PackageReleaseNotes>
            Release 2.1.0:
            - Updated the StyleCop.Analyzers and SonarAnalyzer.CSharp libraries versions and fixed the associated issues
            - Updated all nuget packages to the latest version possible
        </PackageReleaseNotes>
        <PackageTags>Extensions; Cache;</PackageTags>
        <IsPackable>true</IsPackable>
    </PropertyGroup>
    
    <ItemGroup>
        <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="7.0.0" />
        <PackageReference Include="System.Runtime.Caching" Version="7.0.0" />
    </ItemGroup>
</Project>
